{
  "name": "sbomify-frontend-components",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint \"core/js/**/*.{js,ts,vue}\" \"sboms/js/**/*.{js,ts,vue}\" \"*.{js,ts}\"",
    "lint-fix": "eslint \"core/js/**/*.{js,ts,vue}\" \"sboms/js/**/*.{js,ts,vue}\" \"*.{js,ts}\" --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,vue,css,scss,html}\" --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit --skipLibCheck -p tsconfig.app.json"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@mdi/font": "^7.4.47",
    "axios": "^1.7.7",
    "chart.js": "^4.4.6",
    "clipboard": "^2.0.11",
    "dotenv": "^16.4.5",
    "highlight.js": "^11.10.0",
    "js-cookie": "^3.0.5",
    "sweetalert2": "^11.15.2",
    "vue": "^3.4.37",
    "vue-chartjs": "^5.3.2",
    "vuetify": "^3.7.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/vue": "^8.1.0",
    "@types/bun": "^1.1.10",
    "@types/js-cookie": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.13.0",
    "jsdom": "^26.1.0",
    "markdownlint-cli": "^0.43.0",
    "prettier": "^3.4.2",
    "typescript": "5.6.2",
    "vite": "^5.4.5",
    "vite-plugin-pwa": "^1.0.0",
    "vitest": "^3.2.3",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.10"
