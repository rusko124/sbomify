{
  "name": "sbomify-frontend-components",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint \"core/js/**/*.{js,ts,vue}\" \"sboms/js/**/*.{js,ts,vue}\" \"*.{js,ts}\"",
    "lint-fix": "eslint \"core/js/**/*.{js,ts,vue}\" \"sboms/js/**/*.{js,ts,vue}\" \"*.{js,ts}\" --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,vue,css,scss,html}\" --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit --skipLibCheck -p tsconfig.app.json",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@mdi/font": "^7.4.47",
    "@types/bun": "^1.2.16",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.0.3",
    "@vitejs/plugin-vue": "^5.2.4",
    "axios": "^1.10.0",
    "chart.js": "^4.5.0",
    "clipboard": "^2.0.11",
    "dotenv": "^16.5.0",
    "highlight.js": "^11.11.1",
    "js-cookie": "^3.0.5",
    "sweetalert2": "^11.22.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-pwa": "^1.0.0",
    "vue": "^3.5.17",
    "vue-chartjs": "^5.3.2",
    "vue-tsc": "^2.2.10",
    "vuetify": "^3.8.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/vue": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^15.15.0",
    "jsdom": "^26.1.0",
    "markdownlint-cli": "^0.43.0",
    "prettier": "^3.5.3",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^9.4.3"
  }
}
